.resultList{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 30rpx;
  border-bottom: 1rpx solid #d4d4d4;
  font-size: 26rpx;
}
.resultList_text{
  display: -webkit-box;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.icon-bofang1{
  font-size: 70rpx;
  color: #d4d4d4;
}