/* pages/search/search.wxss */
.history_head {
  display: flex;
  justify-content: space-between;
  font-size: 34rpx;
  align-items: center;
  padding: 0 30rpx;
  font-weight: 900;
}

.icon-lajitong {
  font-size: 60rpx;
  font-weight: 1;
}

.history_main {
  padding: 10rpx 30rpx;
  display: flex;
  flex-wrap: wrap
}

.history_main view {
  border-radius: 30rpx;
  padding: 10rpx;
  background-color: #f8f8f8;
  margin: 0 10rpx;
  font-size: 26rpx;
}
.topList{
  padding: 10rpx 30rpx;
}
.topList_head text{
  font-size: 34rpx;
  font-weight: 900;
}
.topList_main{
  display: flex;
  font-size: 36rpx;
  align-items: center;
  padding: 10rpx 0;
}
.topList_main_num{
  width: 100rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  font-size: 40rpx;
}

.topList_main_text{
  width: 75%;
}
